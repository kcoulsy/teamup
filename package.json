{
  "name": "teamup",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.18.9",
    "@tanstack/react-query": "^4.1.3",
    "@tanstack/react-query-devtools": "^4.2.1",
    "@vitejs/plugin-react-refresh": "1.3.1",
    "antd": "4.5.4",
    "history": "5.0.0",
    "node-sass": "4.14.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "vite": "2.3.2",
    "vite-plugin-svgr": "0.3.0",
    "zod": "^3.18.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "lint": "eslint ./src ./server",
    "lint:fix": "eslint --fix ./src ./server",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md|ts|tsx)\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.9",
    "@types/node": "14.0.27",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/parser": "3.9.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.5.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "sass": "^1.54.0",
    "ts-jest": "24.2.0",
    "typescript": "3.9.7",
    "vitest": "^0.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
